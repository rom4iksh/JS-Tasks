* {
    box-sizing: border-box;
    padding: 0;
    margin: 0 auto;
    font-family: sans-serif;
}

.container {
    width: 1000px;
    margin: 30px auto;
}

.input {
    width: 200px;
    height: 20px;
    margin-right: 10px;
}

.btn {
    padding: 5px 15px;
}

ul {
    line-height: 25px;
}

li {
    list-style: none;
}

li input {
    margin: 0 20px;
}

li input:last-child {
    padding: 0px 5px;
}
